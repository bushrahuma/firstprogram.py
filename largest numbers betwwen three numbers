n1 = int(input("Enter first number: "))
n2 = int(input("Enter second number: "))
n3 = int(input("Enter third number: "))

if (n1 > n2) and (n1 > n3):
    largest = n1
elif (n2 > n1) and (n2 > n3):
    largest = n2
else:
    largest = n3

print("The largest number is", largest)

def add_numbers(a,b):
    sum = a+b

    n1 = 5
    n2 = 9
    print(sum)
    print("sum is:", add_numbers(n1 , n2))
